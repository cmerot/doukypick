{
	"name": "doukypick",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"dev:tina": "tinacms dev -c \"vite dev\"",
		"build:tina": "tinacms build && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006 --ci --disable-telemetry",
		"build-storybook": "storybook build -o static/storybook --disable-telemetry"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@internationalized/date": "^3.8.1",
		"@lucide/svelte": "^0.544.0",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-a11y": "^9.1.10",
		"@storybook/addon-docs": "^9.1.10",
		"@storybook/addon-svelte-csf": "^5.0.9",
		"@storybook/addon-vitest": "^9.1.10",
		"@storybook/sveltekit": "^9.1.10",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@tinacms/cli": "^1.11.0",
		"@types/node": "^22",
		"@vitest/browser": "^3.2.3",
		"bits-ui": "^2.11.0",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-storybook": "^9.1.10",
		"eslint-plugin-svelte": "^3.0.0",
		"formsnap": "^2.0.1",
		"globals": "^16.0.0",
		"lucide-svelte": "^0.544.0",
		"mdsvex": "^0.12.3",
		"mode-watcher": "^1.1.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^9.1.10",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tinacms": "^2.9.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0",
		"zod": "^4.1.11"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"ignoredBuiltDependencies": [
			"better-sqlite3"
		]
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.58.0",
		"@vercel/blob": "^2.0.0"
	}
}
